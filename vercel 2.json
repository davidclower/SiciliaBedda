{
  "version": 2,
  "builds": [
    { "src": "api/contact.js", "use": "@vercel/node" },
    { "src": "api/create-checkout-session.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/contact", "dest": "/api/contact.js" },
    { "src": "/api/create-checkout-session", "dest": "/api/create-checkout-session.js" },
    { "src": "/(.*\\.(html|js|css|png|jpg|jpeg|gif|svg|ico|json))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
